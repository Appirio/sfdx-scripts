{
  "name": "@appirio/sfdx-scripts",
  "description": "Define named scripts to run at various times in the lifecycle of your sfdx project.",
  "version": "0.1.5",
  "author": "Roarke Lynch",
  "bugs": {
    "url": "https://github.com/Appirio/sfdx-scripts/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "1",
    "@salesforce/command": "^2.1.0",
    "@salesforce/core": "^2.1.2",
    "chalk": "2.4.2",
    "string-argv": "^0.3.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/test": "^1.2.5",
    "@salesforce/dev-config": "^1.5.0",
    "@sinonjs/samsam": "^3.3.3",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "chai": "^4",
    "globby": "^8.0.2",
    "mocha": "^5",
    "nyc": "^14.1.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Appirio/sfdx-scripts",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "run": {
        "description": "Define and execute scripts from Sfdx."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appirio/sfdx-scripts.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepare": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
